{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      
    },
    "overrides": [
      {
        "files": ["*.js"],
        "rules": {
          "quotes": ["warn", "single"],
          "semi": ["warn", "never"],
          "no-unused-vars": 0
        }
      }
    ]
}
